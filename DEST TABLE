  CREATE TABLE "DEST" 
   (	"SURROGATE" NUMBER INCREMENT BY 1 START WITH 1   NOT NULL ENABLE, 
	"ID" NUMBER(14,0) NOT NULL ENABLE, 
	"NAME" VARCHAR2(20), 
	"ADDRESS" NVARCHAR2(20), 
	"IND" NUMBER NOT NULL ENABLE, 
	"STARTDATE" TIMESTAMP (6) NOT NULL ENABLE, 
	"ENDDATE" DATE DEFAULT '12/31/5000', 
	 CONSTRAINT "DEST_PK" PRIMARY KEY ("SURROGATE")
  USING INDEX  ENABLE
   ) ;
